<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigraph"
    app:startDestination="@id/adFragment">

    <fragment
        android:id="@+id/adFragment"
        android:name="com.g2.runningback.AdFragment"
        android:label="fragment_ad"
        tools:layout="@layout/fragment_ad" >
        <action
            android:id="@+id/action_adFragment_to_promotionFragment"
            app:destination="@id/promotionFragment" />
    </fragment>
    <fragment
        android:id="@+id/userFragment"
        android:name="com.g2.runningback.UserFragment"
        android:label="fragment_user"
        tools:layout="@layout/fragment_user" />
    <fragment
        android:id="@+id/productFragment"
        android:name="com.g2.runningback.ProductFragment"
        android:label="fragment_product"
        tools:layout="@layout/fragment_product" >
        <action
            android:id="@+id/action_productFragment_to_newproductFragment"
            app:destination="@id/newproductFragment" />
        <action
            android:id="@+id/action_productFragment_to_updateproductFragment"
            app:destination="@id/updateproductFragment" />
    </fragment>
    <fragment
        android:id="@+id/orderFragment"
        android:name="com.g2.runningback.OrderFragment"
        android:label="fragment_order"
        tools:layout="@layout/fragment_order" />
    <fragment
        android:id="@+id/saleFragment"
        android:name="com.g2.runningback.SaleFragment"
        android:label="fragment_sale"
        tools:layout="@layout/fragment_sale" />
    <fragment
        android:id="@+id/newproductFragment"
        android:name="com.g2.runningback.NewproductFragment"
        android:label="fragment_newproduct"
        tools:layout="@layout/fragment_newproduct" >
        <action
            android:id="@+id/action_newproductFragment_to_newproduct2Fragment"
            app:destination="@id/newproduct2Fragment" />
    </fragment>
    <fragment
        android:id="@+id/updateproductFragment"
        android:name="com.g2.runningback.UpdateproductFragment"
        android:label="fragment_updateproduct"
        tools:layout="@layout/fragment_updateproduct" />
    <fragment
        android:id="@+id/promotionFragment"
        android:name="com.g2.runningback.PromotionFragment"
        android:label="fragment_promotion"
        tools:layout="@layout/fragment_promotion" />
    <fragment
        android:id="@+id/newproduct2Fragment"
        android:name="com.g2.runningback.Newproduct2Fragment"
        android:label="fragment_newproduct2"
        tools:layout="@layout/fragment_newproduct2" />
</navigation>