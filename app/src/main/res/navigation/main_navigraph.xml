<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigraph"
    app:startDestination="@id/adFragment">

    <fragment
        android:id="@+id/adFragment"
        android:name="com.g2.runningback.AdFragment"
        android:label="fragment_ad"
        tools:layout="@layout/fragment_ad" >
        <action
            android:id="@+id/action_adFragment_to_promotionFragment"
            app:destination="@id/promotionFragment" />
        <action
            android:id="@+id/action_adFragment_to_userFragment"
            app:destination="@id/userFragment" />
    </fragment>
    <fragment
        android:id="@+id/userFragment"
        android:name="com.g2.runningback.User.UserFragment"
        android:label="fragment_user"
        tools:layout="@layout/fragment_user" >
        <action
            android:id="@+id/action_userFragment_to_userDetailFragment"
            app:destination="@id/userDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/productFragment"
        android:name="com.g2.runningback.prod.ProductFragment"
        android:label="fragment_product"
        tools:layout="@layout/fragment_product" >
        <action
            android:id="@+id/action_productFragment_to_productInsertFragment3"
            app:destination="@id/productInsertFragment3" />
        <action
            android:id="@+id/action_productFragment_to_productUpdateFragment"
            app:destination="@id/productUpdateFragment" />
        <action
            android:id="@+id/action_productFragment_to_serverFragment"
            app:destination="@id/serverFragment" />
    </fragment>
    <fragment
        android:id="@+id/orderFragment"
        android:name="com.g2.runningback.OrderFragment"
        android:label="fragment_order"
        tools:layout="@layout/fragment_order" />
    <fragment
        android:id="@+id/saleFragment"
        android:name="com.g2.runningback.SaleFragment"
        android:label="fragment_sale"
        tools:layout="@layout/fragment_sale" />
    <fragment
        android:id="@+id/promotionFragment"
        android:name="com.g2.runningback.PromotionFragment"
        android:label="fragment_promotion"
        tools:layout="@layout/fragment_promotion" />
    <fragment
        android:id="@+id/productInsertFragment3"
        android:name="com.g2.runningback.prod.ProductInsertFragment"
        android:label="fragment_product_insert"
        tools:layout="@layout/fragment_product_insert" >
        <action
            android:id="@+id/action_productInsertFragment3_to_productInsertPTFragment"
            app:destination="@id/productInsertPTFragment" />
    </fragment>
    <fragment
        android:id="@+id/productInsertPTFragment"
        android:name="com.g2.runningback.prod.ProductInsertPTFragment"
        android:label="fragment_product_insert_two"
        tools:layout="@layout/fragment_product_insert_two" >
        <action
            android:id="@+id/action_productInsertPTFragment_to_productFragment"
            app:destination="@id/productFragment" />
    </fragment>
    <fragment
        android:id="@+id/productUpdateFragment"
        android:name="com.g2.runningback.prod.ProductUpdateFragment"
        android:label="fragment_product_update"
        tools:layout="@layout/fragment_product_update" />
    <fragment
        android:id="@+id/serverFragment"
        android:name="com.g2.runningback.Service.ServerFragment"
        android:label="fragment_server"
        tools:layout="@layout/fragment_server" >
        <action
            android:id="@+id/action_serverFragment_to_serverDetailFragment"
            app:destination="@id/serverDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/serverDetailFragment"
        android:name="com.g2.runningback.Service.ServerDetailFragment"
        android:label="fragment_server_detail"
        tools:layout="@layout/fragment_server_detail" />
    <fragment
        android:id="@+id/userDetailFragment"
        android:name="com.g2.runningback.User.UserDetailFragment"
        android:label="fragment_user_detail"
        tools:layout="@layout/fragment_user_detail" />

</navigation>